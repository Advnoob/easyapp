<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="com.customer.fawvw.issues.commands.category">
      </category>
      <command
            id="com.customer.fawvw.issue.TCPprojectCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="项目属性扩展">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueSingleReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="单个问题汇报报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueSingleInfoReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="单个问题信息报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="问题统计报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueForPreReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="预批量车报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueShiftReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="零件批次问题报表">
      </command>
      <command
            id="com.customer.fawvw.issues.ImPartIssueReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="重点件验证问题报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueRelComponentReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="与专业编码相关的问题报表">
      </command>
      <command
            id="com.customer.fawvw.issues.IssueStatusReportCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            name="问题状态统计报表">
      </command>
      <command
            name="SearchCommand"
            categoryId="com.customer.fawvw.issues.commands.category"
            id="com.customer.fawvw.search.SearchCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.customer.fawvw.issues.handler.TCPprojectHandler"
            commandId="com.customer.fawvw.issue.TCPprojectCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueSingleReportHandler"
            commandId="com.customer.fawvw.issues.IssueSingleReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueSingleInfoReportHandler"
            commandId="com.customer.fawvw.issues.IssueSingleInfoReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueReportHandler"
            commandId="com.customer.fawvw.issues.IssueReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueForPreReportHandler"
            commandId="com.customer.fawvw.issues.IssueForPreReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueShiftReportHandler"
            commandId="com.customer.fawvw.issues.IssueShiftReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.ImPartIssueReportHandler"
            commandId="com.customer.fawvw.issues.ImPartIssueReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueRelComponentReportHandler"
            commandId="com.customer.fawvw.issues.IssueRelComponentReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.issues.handler.IssueStatusReportHandler"
            commandId="com.customer.fawvw.issues.IssueStatusReportCommand">
      </handler>
      <handler
            class="com.customer.fawvw.search.handler.LoadSearchHandler"
            commandId="com.customer.fawvw.search.SearchCommand">
      </handler>
   </extension>
   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="com.customer.fawvw.issue.TCPprojectCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="FAW-VW"
               mnemonic="M"
               id="com.customer.rac.helloworld.menus.sampleMenu">
	            <command
	                  commandId="com.customer.fawvw.issue.TCPprojectCommand"
	                  icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                  id="com.customer.fawvw.issue.TCPprojectCommand1"
	                  style="push">
	            </command>
	            <menu
	                  label="问题管理">
	               <command
	                     commandId="com.customer.fawvw.issues.IssueSingleReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport1">
	               </command>
	               <command
	                     commandId="com.customer.fawvw.issues.IssueSingleInfoReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport2">
	               </command>
				   <command
                     	commandId="com.customer.fawvw.issues.IssueReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport3">
              	   </command>
		           <command
	                     commandId="com.customer.fawvw.issues.IssueForPreReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport4">
	               </command>
	               <command
	                     commandId="com.customer.fawvw.issues.IssueShiftReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport5">
	               </command>
	               <command
	                     commandId="com.customer.fawvw.issues.ImPartIssueReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport6">
	               </command>
	               <command
	                     commandId="com.customer.fawvw.issues.IssueRelComponentReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport7">
	               </command>
	               <command
	                     commandId="com.customer.fawvw.issues.IssueStatusReportCommand"
	                     icon="platform:/plugin/com.customer.fawvw.issues/coins/issueMenu.png"
	                     id="IssueReport8">
	               </command>
	            </menu>

         </menu>
      </menuContribution>

	<!-- 搜索按钮 -->
      <menuContribution
            locationURI="toolbar:com.teamcenter.rac.ui.views.TCComponentView">
         <command
              commandId="com.customer.fawvw.search.SearchCommand"
              icon="platform:/plugin/com.customer.fawvw.issues/coins/load.png"
              label="ReSearch"
              style="push"
              tooltip="ReSearch">
            <visibleWhen>
                <iterate>
                    <adapt type="com.teamcenter.rac.kernel.TCComponent">
                        <test
                            property="com.teamcenter.rac.kernel.TCComponent.typeClass"
                            value="SavedSearch,">
                        </test>
                    </adapt>
                </iterate>
            </visibleWhen>
        </command>
      </menuContribution>
      
   </extension>
   
   <!-- 修改Item图标  -->
   <extension point="com.teamcenter.rac.common.tcLabelProviderDelegates">
        <labelProviderDelegate>
            <typeDelegate class="com.customer.fawvw.issues.status.UpdateItemPic" 
            	parentType="FV9IssueRevision" priority="100">                      
            </typeDelegate>
        </labelProviderDelegate>
    </extension>  

</plugin>
